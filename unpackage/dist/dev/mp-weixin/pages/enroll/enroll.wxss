page {
  height: 100%;
  width: 100%;
  background-color: #f6f6f7;
}
.placeholder-style {
  font-size: 28rpx;
}
.enroll {
  width: 100%;
  box-sizing: border-box;
  padding: 100rpx 100rpx;
}
.enroll .enroll-header {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  margin-bottom: 50rpx;
}
.enroll .enroll-body {
  width: 100%;
  background-color: #f6f6f7;
}
.enroll .enroll-body .enroll-body-usrname,
.enroll .enroll-body .enroll-body-usrpwd {
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  border: 1px solid #c9c9c9;
  border-radius: 40rpx;
  margin-bottom: 50rpx;
}
.enroll .enroll-body .enroll-body-usrname .enroll-body-usrname-input,
.enroll .enroll-body .enroll-body-usrpwd .enroll-body-usrname-input,
.enroll .enroll-body .enroll-body-usrname .enroll-body-usrpwd-input,
.enroll .enroll-body .enroll-body-usrpwd .enroll-body-usrpwd-input {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding-left: 20rpx;
  border-radius: 40rpx;
  color: #333333;
  background-color: #FFFFFF;
}
.enroll .enroll-footer {
  position: relative;
  width: 100%;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.enroll .enroll-footer .enroll-footer-enrollbtn {
  box-sizing: border-box;
  height: 100%;
  width: 150rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  border: 1px solid #c9c9c9;
  background-color: #FFFFFF;
}
.enroll .enroll-footer .enroll-footer-reg {
  position: absolute;
  right: 0;
  top: 90rpx;
  height: 100%;
  font-size: 28rpx;
  color: #57bd6a;
}

