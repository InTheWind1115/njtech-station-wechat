page {
  height: 100%;
  width: 100%;
  background-color: #f6f6f7;
}
.placeholder-style {
  font-size: 28rpx;
}
.register {
  width: 100%;
  box-sizing: border-box;
  padding: 100rpx 100rpx;
}
.register .register-header {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  margin-bottom: 50rpx;
}
.register .register-body {
  width: 100%;
  background-color: #f6f6f7;
}
.register .register-body .register-body-usrname,
.register .register-body .register-body-usrpwd,
.register .register-body .register-body-usrpwd-confirm,
.register .register-body .register-body-phone,
.register .register-body .register-body-code {
  position: relative;
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  border: 2rpx solid #c9c9c9;
  border-radius: 40rpx;
  margin-bottom: 50rpx;
}
.register .register-body .register-body-usrname input,
.register .register-body .register-body-usrpwd input,
.register .register-body .register-body-usrpwd-confirm input,
.register .register-body .register-body-phone input,
.register .register-body .register-body-code input {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding-left: 20rpx;
  border-radius: 40rpx;
  color: #333333;
  background-color: #FFFFFF;
}
.register .register-body .register-body-usrname .register-body-code-btn,
.register .register-body .register-body-usrpwd .register-body-code-btn,
.register .register-body .register-body-usrpwd-confirm .register-body-code-btn,
.register .register-body .register-body-phone .register-body-code-btn,
.register .register-body .register-body-code .register-body-code-btn {
  position: absolute;
  box-sizing: border-box;
  top: 4rpx;
  right: 4rpx;
  height: 70rpx;
  width: 200rpx;
  line-height: 70rpx;
  border-radius: 35rpx;
  text-align: center;
  background-color: blue;
  font-size: 28rpx;
  background-color: #00a1d6;
  color: #FFFFFF;
  z-index: 100;
}
.register .register-body .register-body-usrname .register-body-usrname-hint,
.register .register-body .register-body-usrpwd .register-body-usrname-hint,
.register .register-body .register-body-usrpwd-confirm .register-body-usrname-hint,
.register .register-body .register-body-phone .register-body-usrname-hint,
.register .register-body .register-body-code .register-body-usrname-hint,
.register .register-body .register-body-usrname .register-body-usrpwd-hint1,
.register .register-body .register-body-usrpwd .register-body-usrpwd-hint1,
.register .register-body .register-body-usrpwd-confirm .register-body-usrpwd-hint1,
.register .register-body .register-body-phone .register-body-usrpwd-hint1,
.register .register-body .register-body-code .register-body-usrpwd-hint1,
.register .register-body .register-body-usrname .register-body-usrpwd-hint2,
.register .register-body .register-body-usrpwd .register-body-usrpwd-hint2,
.register .register-body .register-body-usrpwd-confirm .register-body-usrpwd-hint2,
.register .register-body .register-body-phone .register-body-usrpwd-hint2,
.register .register-body .register-body-code .register-body-usrpwd-hint2,
.register .register-body .register-body-usrname .register-body-phone-hint,
.register .register-body .register-body-usrpwd .register-body-phone-hint,
.register .register-body .register-body-usrpwd-confirm .register-body-phone-hint,
.register .register-body .register-body-phone .register-body-phone-hint,
.register .register-body .register-body-code .register-body-phone-hint,
.register .register-body .register-body-usrname .register-body-code-hint,
.register .register-body .register-body-usrpwd .register-body-code-hint,
.register .register-body .register-body-usrpwd-confirm .register-body-code-hint,
.register .register-body .register-body-phone .register-body-code-hint,
.register .register-body .register-body-code .register-body-code-hint {
  position: absolute;
  font-size: 26rpx;
  top: 80rpx;
  left: 20rpx;
  color: #f45d90;
  text-align: center;
}
.register .register-footer {
  position: relative;
  width: 100%;
  height: 80rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.register .register-footer .register-footer-registerbtn {
  box-sizing: border-box;
  height: 100%;
  width: 150rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  border: 1px solid #c9c9c9;
  background-color: #FFFFFF;
}
.register .register-footer .register-footer-reg {
  position: absolute;
  right: 0;
  top: 90rpx;
  height: 100%;
  font-size: 28rpx;
  color: #57bd6a;
}

